{
  "name": "admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc --watch",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "docker:build": "docker build -t quiz-app-admin .",
    "docker:run": "docker run -p 5173:5173 quiz-app-admin",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:staging": "npm run build && npm run test:run && echo 'Deploy to staging'",
    "deploy:production": "npm run build && npm run test:run && echo 'Deploy to production'",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "setup": "npm install && npm run typecheck",
    "dev:mock": "vite --mode mock",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "health": "curl -f http://localhost:5173 || exit 1",
    "monitor": "nodemon --exec 'npm run health' --watch src",
    "generate:component": "npx plop component",
    "generate:page": "npx plop page",
    "generate:service": "npx plop service"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
